# Ignore node_modules and build files in both frontend and backend
frontend/node_modules
backend/node_modules

# Ignore build output
frontend/build
backend/build

# Ignore environment variable files in both frontend and backend
frontend/.env.local
frontend/.env.development.local
frontend/.env.test.local
frontend/.env.production.local
backend/.env.local
backend/.env.development.local
backend/.env.test.local
backend/.env.production.local

# Ignore testing coverage reports
frontend/coverage
backend/coverage

# Ignore log files
frontend/npm-debug.log*
backend/npm-debug.log*

# Ignore package lock files if you want to use yarn instead of npm
frontend/yarn-debug.log*
backend/yarn-debug.log*
